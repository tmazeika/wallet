    "@types/gulp": "^4.0.6",
    "@types/gulp-sass": "^4.0.0",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/gulp-util": "^3.0.34",
    "@types/mongodb": "^3.1.28",
    "@types/node": "^12.0.10",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "debug": "^4.1.1",
    "del": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
