{
  "name": "wallet",
  "scripts": {
    "dev": "concurrently -k \"gulp watch\" \"NODE_ENV=development DEBUG=wallet:* tsnd --respawn --transpile-only -r dotenv/config src/app.ts\"",
    "prod": "gulp --production && NODE_ENV=production DEBUG=wallet:* ts-node -r dotenv/config src/app.ts",
    "dev:docker": "docker-compose up",
    "prod:docker": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up"
  },
